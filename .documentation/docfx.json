            "**/*.csproj"
          ]
        },
        {
          "src": "../modules/storage-csharp/Storage",
          "files": [
            "**/*.csproj"
          ]
        },
        {
          "src": "../modules/functions-csharp/Functions",
          "files": [
            "**/*.csproj"
          ]
        },
        {
